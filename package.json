{
  "name": "web",
  "version": "1.0.0",
  "description": "UPchieve web client",
  "author": "Corey <corey.johns@believeachieve.com>",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:staging": "vue-cli-service build --mode=staging",
    "lint": "vue-cli-service lint",
    "lint:prod": "vue-cli-service lint --mode=production",
    "e2e": "vue-cli-service test:e2e --mode=development",
    "test": "vue-cli-service test:unit",
    "clean-slate": "npx rimraf node_modules dist && npm install"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "core-js": "^2.6.5",
    "jquery": "^3.4.1",
    "lodash": "^4.17.14",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "validator": "^6.3.0",
    "vue": "^2.6.10",
    "vue-headful": "^2.0.1",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.3",
    "vue-socket.io": "^2.1.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-e2e-cypress": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-plugin-unit-jest": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^9.2.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "simple-progress-webpack-plugin": "^1.1.2",
    "style-resources-loader": "^1.2.1",
    "vue-template-compiler": "^2.6.10"
  },
  "engines": {
    "node": ">= 11.7.0",
    "npm": ">= 6.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
